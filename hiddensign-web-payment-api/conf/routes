# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
# authorization
POST          /account           com.hiddensign.web.payment.rest.api.controllers.AccountController.create()
GET           /account/:accId    com.hiddensign.web.payment.rest.api.controllers.AccountController.find(accId: Long)
POST          /account/transfer  com.hiddensign.web.payment.rest.api.controllers.AccountController.transfer()

# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file                                            controllers.Assets.at(path="/public", file)

    